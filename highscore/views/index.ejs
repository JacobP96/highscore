<section>
  <link rel="stylesheet" href="/stylesheets/style.css" />
  <% allHighscores.forEach(higscore => { %>
  <div class="highscore">
    <div class="container">
      <div class="row">
        <div class="col-sm"><%= higscore.game %></div>
        <div class="col-sm"><%= higscore.points %></div>
      </div>
      <div class="row">
        <div class="col-sm">
          <%= higscore.player %>,<%= higscore.score_date %>
        </div>

        <% }) %>
      </div>
    </div>
  </div>
</section>
