<section class="search-results mb-4">
  <div class="text-center mb-4">Hittade <%= products.length %> produkter</div>
  <div class="highscore">
    <a href=""></a>
    <div class="container">
      <div class="row">
        <% products.forEach(product => { %>
        <div class="game col-sm"><%= product.title %></div>
        <div class="col-sm"><%= product.image_url %></div>
      </div>
      <div class="row">
        <div class="col-sm">
          <%= product.genre %>,<%= product.release_date %>
        </div>
      </div>
    </div>
  </div>
  <% }) %>
</section>
